issn = {2059-7983},
journal = {Acta Crystallographica Section D Structural Biology},
keywords = {ARP / wARP,loop building,macromolecular crystallography,model building,sequence similarity},
month = {mar},
number = {3},
pages = {248--260},
pmid = {32133989},
publisher = {International Union of Crystallography},
title = {{The use of local structural similarity of distant homologues for crystallographic model building from a molecular-replacement solution}},
url = {http://scripts.iucr.org/cgi-bin/paper?S2059798320000455},
volume = {76},
year = {2020}
}
@article{White2020,
abstract = {Ruthenium-catalysed azide–alkyne cycloaddition (RuAAC) provides access to 1,5-disubstituted 1,2,3-triazole motifs in peptide engineering applications. However, investigation of this motif as a disulfide mimetic in cyclic peptides has been limited, and the structural consequences remain to be studied. We report synthetic strategies to install various triazole linkages into cyclic peptides through backbone cyclisation and RuAAC cross-linking reactions. These linkages were evaluated in four serine protease inhibitors based on sunflower trypsin inhibitor-1. NMR and X-ray crystallography revealed exceptional consensus of bridging distance and backbone conformations (RMSD<0.5 {\AA}) of the triazole linkages compared to the parent disulfide molecules. The triazole-bridged peptides also displayed superior half-lives in liver S9 stability assays compared to disulfide-bridged peptides. This work establishes a foundation for the application of 1,5-disubstituted 1,2,3-triazoles as disulfide mimetics.},
