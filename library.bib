@article{Petsko1975,
abstract = {A procedure has been developed for maintaining the integrity of protein crystals at sub-zero temperatures. It involves the replacement of the normal crystal mother liquor with salt-free aqueous/organic liquids of low freezing point. Detailed knowledge of the physical chemical properties of these mixed solvents permits conditions to be chosen that maximize the similarity between their micro-environments and that provided by the normal mother liquor. Since the mixed solvents remain fluid at very low temperatures, it is possible to diffuse substrates into the crystals in the cold. Results have been obtained for a dozen different crystalline proteins, demonstrating that the crystals diffract to high resolution after repeated cooling to below-70°C. At low temperatures radiation damage to all the crystals is negligible, and for two proteins there is an improvement in the intensities of high resolution reflections. The technique requires no special equipment, does not change unit cell dimensions, and does not demand cross-linking the crystals.},
annote = {Desarrolla la crio-protecci{\'{o}}n, iniciada por Haas entre 7 y 5 a{\~{n}}os atr{\'{a}}s. Demuestra las ventajas de la t{\'{e}}cnica para una docena de prote{\'{i}}nas. Entre las ventajas destaca el nulo da{\~{n}}o por radiaci{\'{o}}n. Tiene una buena revisi{\'{o}}n de los estudios realizados hasta la {\'{e}}poca: 1966Low, 1968Haas, 1970Haas, 1973Thomaneck.},
author = {Petsko, Gregory A},
doi = {10.1016/0022-2836(75)90167-9},
file = {:home/murphy/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Petsko - 1975 - Protein crystallography at sub-zero temperatures Cryo-protective mother liquors for protein crystals.pdf:pdf},
issn = {00222836},
journal = {Journal of Molecular Biology},
month = {aug},
number = {3},
pages = {381--392},
title = {{Protein crystallography at sub-zero temperatures: Cryo-protective mother liquors for protein crystals}},
url = {https://linkinghub.elsevier.com/retrieve/pii/0022283675901679},
volume = {96},
year = {1975}
}
@article{Phillips1976,
abstract = {X-ray diffraction photographs of protein single crystals have been obtained using synchrotron radiation produced by an electron-positron storage ring. The diffracted intensities observed with this unconventional source are a factor of at least 60 greater than those obtained with a sealed x-ray tube using the same crystal and instrumental parameters. Diffraction data have been collected by the precession method to higher resolution and using smaller protein crystals than would have been possible with a conventional source. The crystal decay rate in the synchrotron beam for several proteins appears to be substantially less than that observed with Ni-fiftered Cu radiation. The tunable nature of 'the source (which allows selective optimization of anomalous contributions to the scattering factors) and the low angular divergence of the beam make the source very useful for single crystal protein diffraction studies. The use of synchrotron radiation as a source for single crystal x-ray diffraction studies has recently been the subject of considerable discussion and controversy. In contrast to conventional sources, the polychromatic radiation produced by synchrotron emission can be monochromated over a large range of wavelengths to give an intense x-ray beam. Questions have been raised about the actual gain in diffracted intensity which is possible over more conventional sources and the ability of biological materials (such as protein crystals) to withstand the high radiation flux.},
annote = {En este art{\'{i}}culo se demuestra que la radiaci{\'{o}}n sincrotr{\'{o}}n es una fuente viable de rayos-X para realizar difracci{\'{o}}n de rayos-X de cristales de prote{\'{i}}na. Adem{\'{a}}s proponen que, en teor{\'{i}}a, es posible colectar datos a tres longitudes de onda para resolver el problema de las fases.},
author = {Phillips, James C and Wlodawer, Alexander and Yevitz, Marguerite M and Hodgson, Keith O.},
doi = {10.1073/pnas.73.1.128},
file = {:home/murphy/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Phillips et al. - 1976 - Applications of synchrotron radiation to protein crystallography preliminary results.pdf:pdf},
issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
month = {jan},
number = {1},
pages = {128--132},
title = {{Applications of synchrotron radiation to protein crystallography: preliminary results.}},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.73.1.128},
volume = {73},
year = {1976}
}
@article{Hendrickson1976,
abstract = {A general model is derived to describe the rate of radiation damage in protein crystals. This model and some of its special cases are tested against the Blake and Phillips data on X-irradiation effects in myoglobin crystals. The results point to a sequential process of damage and suggest an improved method for correcting diffraction data for the effects of radiation damage. Exposure of protein crystals to X-rays for the measurement of diffraction intensities causes appreciable damage, as is manifest in the deterioration of diffraction patterns. On average, the intensity of reflections decreases with exposure time and does so most markedly at higher scattering angles. Irradiation of myoglobin crystals has an effect equivalent to the damaging or destroying of 160 molecules by each CuKa quantum that is absorbed (Blake & Phillips, 1962). Radiation damage is even more severe for many other proteins. The study by Blake & Phillips (1962) suggests that after a given dose of radiation, the diffraction pattern derives from a crystal which consists of three components: an essentially undamaged fraction A,, a highly disordered fraction A, which contributes significantly only at low angles, and an effectively amorphous fraction A, which no longer scatters coherently. If the disordering is assumed to be random, with mean-square atomic displacement of D/S 'IT, then the scattering from disordered molecules will be attenuated relative to that from native molecules by a factor exp(-Ds2), where s = si&/h. Thus the relation of intensity, I(t), of diffraction after exposure t to the initial intensity is governed by the law I(t)/I(O) = A,(t) + A,(t) exp(-Ds2). (1)},
author = {Hendrickson, Wayne A.},
doi = {10.1016/0022-2836(76)90271-0},
file = {:home/murphy/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Hendrickson - 1976 - Radiation damage in protein crystallography.pdf:pdf},
issn = {00222836},
journal = {Journal of Molecular Biology},
month = {sep},
number = {3},
pages = {889--893},
title = {{Radiation damage in protein crystallography}},
url = {https://linkinghub.elsevier.com/retrieve/pii/0022283676902710},
volume = {106},
year = {1976}
}
