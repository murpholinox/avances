issn = {0909-0495},
journal = {Journal of Synchrotron Radiation},
keywords = {Freeze substitution,Radiation damage,Resin embedding},
month = {jan},
number = {1},
pages = {128--132},
pmid = {17211079},
title = {{Plastic-embedded protein crystals}},
url = {http://scripts.iucr.org/cgi-bin/paper?S0909049506043111},
volume = {14},
year = {2007}
}
@article{Southworth-Davies2007,
abstract = {Radiation damage continues to present a problem to crystallographers using cryocooled protein crystals at third-generation synchrotrons. Free-radical scavengers have been suggested as a possible means of reducing the rate of this damage. The screening of a large number of potential radioprotectants was undertaken with an online microspectrophotometer using cystine and cysteine to model protein disulfide bonds and thiol groups, respectively. Oxidized $\alpha$-lipoic acid was tested as a possible model disulfide bond. The evidence for the effectiveness of ascorbate as a radioprotectant was strengthened, and quinone, 2,2,6,6-tetramethyl-4-piperidone, and reduced dithiothreitol showed promise as radioprotectants. {\textcopyright} 2007 International Union of Crystallography Printed in Singapore - all rights reserved.},
