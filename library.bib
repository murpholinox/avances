@article{Helliwell1988,
abstract = {Protein crystals have been found to be more perfect as measured by their X-ray reflection rocking widths using collimated synchrotron radiation. The use of this parameter to assess the perfection of crystals grown by new methods, or in new environments such as microgravity, is important. Irradiation of protein crystals with X-rays almost invariably leads to a loss of perfection. The effects are physical and chemical. The use of rapid exposures and short wavelengths ameliorates the effects and allows more data to be collected per sample. The interrelation of these effects is discussed and crystallographic results on des-pentapeptide insulin using synchrotron radiation are compared with the results of Burley, Petsko and Ringe (Biochemistry, in press) on ribonuclease and a conventional X-ray source.},
