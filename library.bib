volume = {26},
year = {2019}
}
@article{Diupotex2019,
abstract = {Parasites have been engineered to express fluorescent reporter proteins, yet the impact of red fluorescent proteins on Leishmania infections remains largely unknown. We analysed the infection outcome of Leishmania mexicana parasites engineered for the constitutive expression of mKate protein and evaluated their immunogenicity in BALB/c mice. Infection of BALB/c mice with mKate transfected L. mexicana (LmexmKate) parasites caused enlarged lesion sizes, leading to ulceration, and containing more parasites, as compared to LmexWT. The mKate protein showed immunogenic properties inducing antibody production against the mKate protein, as well as enhancing antibody production against the parasite. The augmented lesion sizes and ulcers, together with the more elevated antibody production, were related to an enhanced number of TNF-$\alpha$ and IL-1$\beta$ producing cells in the infected tissues. We conclude that mKate red fluorescent protein is an immunogenic protein, capable of modifying disease evolution of L. mexicana.},
author = {Diupotex, Mariana and Mart{\'{i}}nez‐Salazar, Mar{\'{i}}a Berenice and Escalona‐Monta{\~{n}}o, Alma Reyna and Zamora‐Chimal, Jaime and Salaiza‐Suazo, Norma and Ruiz‐Remigio, Adriana and Rold{\'{a}}n‐Salgado, Abigail and Aguirre‐Garc{\'{i}}a, Mar{\'{i}}a Magdalena and Mart{\'{i}}nez‐Calvillo, Santiago and Gayt{\'{a}}n, Paul and Becker, Ingeborg},
