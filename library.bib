file = {:home/murphy/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Nannenga, Bu, Shi - 2018 - The Evolution and the Advantages of MicroED.pdf:pdf},
issn = {2296-889X},
journal = {Frontiers in Molecular Biosciences},
keywords = {Cryo-EM,Crystallography,Dynamic scattering,MicroED,Radiation damage},
