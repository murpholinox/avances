file = {:home/murphy/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/de Sanctis et al. - 2016 - Radiation-damage-induced phasing a case study using UV irradiation with light-emitting diodes.pdf:pdf},
issn = {2059-7983},
journal = {Acta Crystallographica Section D Structural Biology},
keywords = {RIP,SIR,UV,disulfide bonds,light-emitting diodes,phasing,radiation damage,ultraviolet light},
