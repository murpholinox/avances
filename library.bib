issn = {00222836},
journal = {Journal of Molecular Biology},
keywords = {Anion binding,Biosensor,Chloride sensor,Green fluorescent protein,Linked equilibrium},
month = {aug},
number = {1},
pages = {157--171},
pmid = {10926499},
publisher = {Academic Press},
title = {{Crystallographic and energetic analysis of binding of selected anions to the yellow variants of green fluorescent protein 1 1Edited by D. C. Rees}},
