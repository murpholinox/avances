file = {:home/murphy/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Kawabata et al. - 2006 - Evaluation of crystalline objects in crystallizing protein droplets based on line-segment information in greysc.pdf:pdf},
issn = {0907-4449},
journal = {Acta Crystallographica Section D Biological Crystallography},
month = {mar},
number = {3},
pages = {239--245},
pmid = {16510970},
title = {{Evaluation of crystalline objects in crystallizing protein droplets based on line-segment information in greyscale images}},
url = {http://scripts.iucr.org/cgi-bin/paper?S0907444905041077},
volume = {62},
year = {2006}
}
@article{Leiros2006,
abstract = {This paper focuses on the radiation-damage effects when applying the same total X-ray dose to protein crystals at different dose rates. These experiments have been performed on both a selenomethionated protein and on bovine trypsin using dose rates that span nearly two orders of magnitude. The results show no clear dose-rate effect on the global indicators of radiation damage, but a small measurable dose-rate effect could be found when studying specific radiation damage. It is hypothesized that this observed dose-rate effect relates to differences in the steady-state free-radical concentration. {\textcopyright} 2006 International Union of Crystallography - all rights reserved.},
author = {Leiros, Hanna-Kirsti S. and Timmins, Joanna and Ravelli, Raimond B. G. and McSweeney, Se{\'{a}}n M.},
