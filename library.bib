journal = {Acta Crystallographica Section D Biological Crystallography},
month = {jan},
number = {1},
pages = {32--47},
pmid = {16369092},
title = {{Cryocooling and radiation damage in macromolecular crystallography}},
url = {http://scripts.iucr.org/cgi-bin/paper?S0907444905034207},
volume = {62},
