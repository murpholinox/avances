issn = {0907-4449},
journal = {Acta Crystallographica Section D Biological Crystallography},
month = {jun},
number = {6},
pages = {1085--1093},
pmid = {15159568},
title = {{Structural effects of radiation damage and its potential for phasing}},
url = {http://scripts.iucr.org/cgi-bin/paper?S0907444904007917},
volume = {60},
year = {2004}
}
