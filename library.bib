pages = {88--97},
pmid = {12718922},
publisher = {Academic Press Inc.},
title = {{The protein as a variable in protein crystallization}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S1047847703000418},
volume = {142},
year = {2003}
}
@article{Borek2003,
