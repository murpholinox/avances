issn = {0006-2960},
journal = {Biochemistry},
month = {aug},
number = {32},
pages = {9759--9765},
title = {{Crystal Structure and Photodynamic Behavior of the Blue Emission Variant Y66H/Y145F of Green Fluorescent Protein}},
