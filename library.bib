@article{Spraggon2002,
abstract = {A system for the automatic categorization of the results of crystallization experiments generated by robotic screening is presented. Images from robotically generated crystallization screens are taken at preset time intervals and analyzed by the computer program Crystal Experiment Evaluation Program (CEEP). This program attempts to automatically categorize the individual crystal experiments into a number of simple classes ranging from clear drop to mountable crystal. The algorithm {\textregistered}rst selects features from the images via edge detection and texture analysis. Classi{\textregistered}cation is achieved via a self-organizing neural net generated from a set of hand-classi{\textregistered}ed images used as a training set. New images are then classi{\textregistered}ed according to this neural net. It is demonstrated that incorporation of time-series information may enhance the accuracy of classi{\textregistered}cation. Preliminary results from the screening of the proteome of Thermotoga maritima are presented showing the utility of the system.},
