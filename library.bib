file = {:home/murphy/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Gotthard et al. - 2019 - Specific radiation damage is a lesser concern at room temperature.pdf:pdf},
issn = {2052-2525},
journal = {IUCrJ},
keywords = {cryocrystallography,room-temperature macromolecular crystallography,specific radiation damage,time-resolved crystallography},
month = {jul},
number = {4},
pages = {665--680},
publisher = {International Union of Crystallography},
title = {{Specific radiation damage is a lesser concern at room temperature}},
url = {http://scripts.iucr.org/cgi-bin/paper?S205225251900616X},
volume = {6},
year = {2019}
}
@article{Holton2019,
abstract = {A synthetic data set demonstrating a particularly challenging case of indexing ambiguity in the context of radiation damage was generated. This set shall serve as a standard benchmark and reference point for the ongoing development of new methods and new approaches to robust structure solution when single-crystal methods are insufficient. Of the 100 short wedges of data, only the first 36 are currently necessary to solve the structure by `cheating', or using the correct reference structure as a guide. The total wall-clock time and number of crystals required to solve the structure without cheating is proposed as a metric for the efficacy and efficiency of a given multi-crystal automation pipeline.},
