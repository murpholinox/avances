issn = {1600-5775},
journal = {Journal of Synchrotron Radiation},
keywords = {Cysteine modification,Enzyme conformational dynamics,Molecular dynamics simulations,Radiation-controlled photo-oxidation,Serial X-ray crystallography},
month = {jul},
number = {4},
pages = {958--966},
pmid = {31274417},
publisher = {International Union of Crystallography},
title = {{Shining light on cysteine modification: connecting protein conformational dynamics to catalysis and regulation}},
