issn = {0021-8898},
journal = {Journal of Applied Crystallography},
keywords = {Monte Carlo simulations,X-ray beamlines,X-ray optics,computer programs},
month = {jun},
number = {3},
pages = {679--696},
publisher = {International Union of Crystallography},
title = {{McXtrace : a Monte Carlo software package for simulating X-ray optics, beamlines and experiments}},
url = {http://scripts.iucr.org/cgi-bin/paper?S0021889813007991},
volume = {46},
year = {2013}
