pages = {610--612},
pmid = {14993709},
publisher = {International Union of Crystallography},
title = {{Novel buffer systems for macromolecular crystallization}},
url = {http://scripts.iucr.org/cgi-bin/paper?S0907444903029640},
volume = {60},
year = {2004}
}
@article{Chayen2004,
abstract = {Protein crystallisation has gained a new strategic and commercial relevance in the post-genomic era because of its pivotal role in structural genomics. Producing high-quality crystals has always been a bottleneck to structure determination and, with the advent of proteomics, this problem is becoming increasingly acute. The task of producing suitable crystals may be tackled using two approaches. The first relies on empirical techniques that are based mainly on trial and error, and what is perceived to be the 'art' of crystallisation. The second approach is aimed at gaining an understanding of the fundamental principles that govern crystallisation; this knowledge may be applied to design experimental methodology for producing high-quality crystals of medical and industrial interest.},
author = {Chayen, Naomi E.},
doi = {10.1016/j.sbi.2004.08.002},
file = {:home/murphy/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Chayen - 2004 - Turning protein crystallisation from an art into a science.pdf:pdf},
issn = {0959440X},
journal = {Current Opinion in Structural Biology},
month = {oct},
number = {5},
pages = {577--583},
pmid = {15465318},
title = {{Turning protein crystallisation from an art into a science}},
url = {https://linkinghub.elsevier.com/retrieve/pii/S0959440X04001368},
volume = {14},
year = {2004}
}
@article{Ramos2004,
abstract = {We report here the construction of a vector derived from pET3-His and pRSET plasmids for the expression and purification of recombinant proteins in Escherichia coli based on T7 phage RNA polymerase. The resulting pAE plasmid combined the advantages of both vectors: small size (pRSET), expression of a short 6XHis tag at N-terminus (pET3-His) and a high copy number of plasmid (pRSET). The small size of the vector (2.8 kb) and the high copy number/cell (200-250 copies) facilitate the subcloning and sequencing procedures when compared to the pET system (pET3-His, 4.6 kb and 40-50 copies) and also result in high level expression of recombinant proteins (20 mg purified protein/liter of culture). In addition, the vector pAE enables the expression of a fusion protein with a minimal amino-terminal hexa-histidine affinity tag (a tag of 9 amino acids using XhoI restriction enzyme for the 5'cloning site) as in the case of pET3-His plasmid and in contrast to proteins expressed by pRSET plasmids (a tag of 36 amino acids using BamHI restriction enzyme for the 5'cloning site). Thus, although proteins expressed by pRSET plasmids also have a hexa-histidine tag, the fusion peptide is much longer and may represent a problem for some recombinant proteins.},
author = {Ramos, CRR and Abreu, Pae and Nascimento, Alto and Ho, PL},
doi = {10.1590/S0100-879X2004000800001},
file = {:home/murphy/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Ramos et al. - 2004 - A high-copy T7 Escherichia coli expression vector for the production of recombinant proteins with a minimal N-term.pdf:pdf},
issn = {0100-879X},
journal = {Brazilian Journal of Medical and Biological Research},
keywords = {Escherichia coli,Expression vector,Immobilized metal affinity chromatography,Protein purification},
month = {aug},
number = {8},
pages = {1103--1109},
title = {{A high-copy T7 Escherichia coli expression vector for the production of recombinant proteins with a minimal N-terminal His-tagged fusion peptide}},
url = {http://www.scielo.br/scielo.php?script=sci_arttext&pid=S0100-879X2004000800001&lng=en&tlng=en},
volume = {37},
year = {2004}
}
@article{Banumathi2004,
abstract = {A detailed analysis of radiation-damage-induced structural and intensity changes is presented on the model protein thaumatin. Changes in reflection intensities induced by irradiation display a parabolic character. The most pronounced structural changes observed were disulfide-bond breakage and associated main-chain and side-chain movements as well as decarboxylation of aspartate and glutamate residues. The structural changes induced on the sulfur atoms were successfully used to obtain high-quality phase estimates via an RIP procedure. Results obtained with ACORN suggest that the contribution originating from the partial structure may play an important role in phasing even at less than atomic resolution. {\textcopyright} 2004 International Union of Crystallography Printed in Denmark - all rights reserved.},
author = {Banumathi, Sankaran and Zwart, Petrus H. and Ramagopal, Udupi A. and Dauter, Miroslawa and Dauter, Zbigniew},
