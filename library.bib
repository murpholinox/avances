doi = {10.1038/319631a0},
file = {:home/murphy/.var/app/com.elsevier.MendeleyDesktop/data/data/Mendeley Ltd./Mendeley Desktop/Downloaded/Stewart, Vigers - 1986 - Electron microscopy of frozen-hydrated biological material.pdf:pdf},
issn = {0028-0836},
journal = {Nature},
month = {feb},
number = {6055},
pages = {631--636},
title = {{Electron microscopy of frozen-hydrated biological material}},
