journal = {Protein Expression and Purification},
keywords = {Absorption spectra,Butyl Sepharose chromatography,Chromophore maturation,GFP-like fluorescent proteins,Green fluorescent protein,Organic extraction,Recombinant protein expression and purification,Two phase water/organic system},
month = {may},
