issn = {0909-0495},
journal = {Journal of Synchrotron Radiation},
keywords = {Chloroperoxidase,Cytochrome P450,Energy dependence,Hydrated electrons,Microspectrophotometer,Myoglobin,Photoreduction,Radiation damage,Radical scavengers,Radiolytic reduction},
month = {jan},
